<div class="classroom-resource-item">
  <span class="group-assignment-icon assignment-icon-group left">
    <%= octicon 'link' %>
  </span>

  <%= link_to stafftools_group_assignment_invitation_path(group_assignment_invitation.id) do %>
    <strong class="search-result-title">Group assignment invitation for <%= group_assignment_invitation.group_assignment_title %></strong>
  <% end %>

  <span class="text-muted css-truncate css-truncate-target">Created <%= local_time_ago(group_assignment_invitation.created_at) %></span>
</div>
